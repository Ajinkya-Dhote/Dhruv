<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" 
xmlns="sap.m" xmlns:f="sap.f" controllerName="els.org.pratideen.controller.App" displayBlock="true" class="sapUiSizeCompact">
		<App id="idAppControl">
			<Page title="{i18n>labelDashboard}">
			<content>
			<List items="{oDashModel>/results}" mode="SingleSelectMaster" selectionChange="onListItemChange">
			<StandardListItem 
				icon="sap-icon://display"				
				title="{oDashModel>Item}"				
			>				
			</StandardListItem>
			</List>
		</content>
		<footer>
			<Toolbar>
				<ToolbarSpacer/>
					<Button text="Edit" press="handleEditPress" />
					<Button text="Busy state" press="handleBusyPress" />
				<ToolbarSpacer/>
			</Toolbar>
		</footer>
			
			</Page>
		</App>
</mvc:View>
