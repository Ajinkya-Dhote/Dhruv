{
  "name": "files-mgr",
  "version": "1.0.0",
  "description": "Building a Node.js/Express RestAPIs that upload/download files to PostgreSQL database using Multer middleware, Sequelize ORM",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint ./app",
    "eslint-fix": "eslint ./app --fix",
    "build": "webpack",
    "prod": "node dist/api.bundle.js",
    "build_prod_run": "npm run build_webcli && npm run prod",
    "webcli": "npm i webpack-cli -g",
    "build_webcli": "npm run webcli && npm run build",
    "run": "node server.js"
  },
  "keywords": [
    "Node.js",
    "Express",
    "PostgreSQL",
    "Multer",
    "Sequelize"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.726.0",
    "cors": "latest",
    "express": "^4.16.3",
    "multer": "^1.3.0",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^4.37.7",
    "swagger-jsdoc": "^1.3.0",
    "swagger-ui-express": "4.1.4",
    "tedious": "^9.0.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",

    "xlsx": "latest",
    "winston": "latest",
    "fs": "latest",
    "winston-daily-rotate-file": "4.5.0"
  },
  "devDependencies": {
    "eslint": "^7.6.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "latest"
  }
}
